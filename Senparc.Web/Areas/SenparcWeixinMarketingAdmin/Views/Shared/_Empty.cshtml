@model Senparc.Areas.Admin.Models.VD.Login_IndexVD
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <meta name="description" content="@ViewBag.Description" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <!--Basic Styles-->
    @Styles.Render("~/backend/css/bootstrap")
    <link id="bootstrap-rtl-link" href="" rel="stylesheet" />

    <!--Beyond styles-->
    @Styles.Render("~/backend/css/beyond")
    <link id="skin-link" href="" rel="stylesheet" type="text/css" />

    <!--Skin Script: Place this script in head to load scripts for skins and rtl support-->
    @Scripts.Render("~/backend/bundles/skin")
    @Scripts.Render("~/backend/bundles/jquery")

    @RenderSection("HeaderContent", false)
</head>
<body>
    @RenderBody()
    <!--Basic Scripts-->

    @Scripts.Render("~/backend/bundles/bootstrap")

    <!--Beyond Scripts-->
    @Scripts.Render("~/backend/bundles/beyond")

    <!--Page Related Scripts-->
    @RenderSection("scripts", required: false)
</body>
</html>
