@model Senparc.Areas.Admin.Models.VD.Home_IndexVD

@{
    ViewBag.Title = "后台管理首页";

}
@section scripts{
}

@using (Html.ContentBox("后台管理首页"))
{
    <div class="content_pad">
        <p>
            当前服务器时间：<span id="serverTime">@DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss")</span> |
            页面执行时间：@((Model.PageEndTime - Model.PageStartTime).TotalSeconds.ToString())秒
        </p>
        <p>
            系统启动时间：@Senparc.Core.Config.SiteConfig.ApplicationStartTime
            | Structure启动时间：@StructureMapControllerFactory.StrcutureMapStartupTime 秒
        </p>
        <p>
            当前缓存策略：@Model.CacheType
        </p>
        <p>
            微信缓存策略：@Model.WechatCacheType.Name
        </p>
    </div>
}